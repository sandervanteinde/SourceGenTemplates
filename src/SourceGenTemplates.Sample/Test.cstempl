::foreach class
    where partial
    as c;

::filename c;
using System;

namespace ::c.Namespace::;

partial class ::c::
{
    public static void SayHello()
    {
        ::foreach c.Properties
            as p;

            ::if p is public;
                Console.WriteLine("Public property ::p::");
            ::else if p is protected;
                Console.WriteLine("Protected property ::p::");
            ::else if p is internal;
                Console.WriteLine("Internal property ::p::");
            ::else;
                Console.WriteLine("Property ::p:: is not public, protected or internal");
            ::end;
        ::end;
    }
}
::end;