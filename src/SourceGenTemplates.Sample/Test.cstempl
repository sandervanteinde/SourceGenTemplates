::foreach var c in  class
    where c is partial;

::filename c;
using System;

namespace ::c.Namespace::;

partial class ::c::
{
    public static void SayHello()
    {
        ::foreach var p in c.Properties;

            ::if p is public or p is protected;
                Console.WriteLine("Public or protected property ::p::");
            ::else if p is internal;
                Console.WriteLine("Internal property ::p::");
            ::else;
                Console.WriteLine("Property ::p:: is not public, protected or internal");
            ::end;
        ::end;
    }
}
::end;